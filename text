스와이퍼

<script>
    function setMenuActiveById(id) {
        $('.menubox > div').removeClass('active');
        $('.menubox > div[data-target="' + id + '"]').addClass('active');
    }

    // 메뉴 클릭 → 해당 id 슬라이드로 이동 + active
    $('.menubox > div').on('click', function () {
        const targetId = $(this).data('target'); // like / good / tmi

        // 슬라이드 인덱스 구하기 (swiper-slide 기준으로)
        const index = $('.rara_Swiper .swiper-slide').index($('#' + targetId));

        // 메뉴 active 즉시 반영
        setMenuActiveById(targetId);

        // loop:true 이므로 slideToLoop 사용
        swiper.slideToLoop(index, 600);
    });

    // 스와이프/네비버튼으로 넘어갈 때도 메뉴 active 자동 갱신
    swiper.on('slideChange', function () {
        const current = swiper.realIndex; // loop일 때는 realIndex가 정답
        const currentId = $('.rara_Swiper .swiper-slide').eq(current).attr('id');
        setMenuActiveById(currentId);
    });

    // 첫 로딩 시 active 세팅
    $(function () {
        const current = swiper.realIndex;
        const currentId = $('.rara_Swiper .swiper-slide').eq(current).attr('id');
        setMenuActiveById(currentId);
    });

</script>